{
  "name": "easy-hooks-ts",
  "version": "0.1.3",
  "description": "A custom hook library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:tsup": "tsup --dts",
    "commit": "git-cz",
    "changeset": "changeset",
    "clean": "rimraf dist",
    "generate": "npx plop --plopfile scripts/cli.js",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint src --ext .ts",
    "lint:eslint:fix": "npm run lint:eslint --fix",
    "lint:prettier": "prettier --check \"src/**/*.ts\"",
    "lint:prettier:fix": "npm run lint:prettier --write",
    "lint:tsc": "tsc --project tsconfig.json --noEmit",
    "test": "vitest --passWithNoTests",
    "release": "changeset publish",
    "version": "changeset version"
  },
  "peerDependencies": {
    "react": "^16.8.0  || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0  || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "@vitejs/plugin-react": "^3.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-10x": "1.5.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-import-helpers": "1.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "3.10.1",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lefthook": "^1.3.10",
    "npm-run-all": "^4.1.5",
    "plop": "^3.1.2",
    "prettier": "2.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "^6.7.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.0.8",
    "vitest": "^0.29.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmartinsoliv/izi-hooks.git"
  },
  "keywords": [
    "custom hook",
    "library",
    "react"
  ],
  "author": "Matheus Martins <mmartinsolliv@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmartinsoliv/izi-hooks/issues"
  },
  "homepage": "https://github.com/mmartinsoliv/izi-hooks#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
